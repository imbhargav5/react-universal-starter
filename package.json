{
  "dependencies": {
    "babel-preset-stage-0": "^6.24.1",
    "express": "^4.15.4",
    "prop-types": "^15.5.10",
    "pug": "^2.0.0-rc.3",
    "react": "next",
    "react-dom": "next",
    "react-redux": "^5.0.6",
    "react-router": "next",
    "react-router-dom": "next",
    "redux": "^3.7.2",
    "styled-components": "^2.1.2"
  },
  "devDependencies": {
    "all-contributors-cli": "^4.4.0",
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-dynamic-import-webpack": "^1.0.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "html-webpack-plugin": "^2.30.1",
    "html-webpack-pug-plugin": "^0.2.1",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "react-hot-loader": "next",
    "rimraf": "^2.6.1",
    "webpack": "^3.5.4",
    "webpack-dev-server": "^2.7.1"
  },
  "name": "react-universal-starter",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Bhargav Ponnapalli <bhargavponnapalli.5@gmail.com> (https://github.com/imbhargav5)",
  "license": "MIT",
  "scripts": {
    "add-contributor": "all-contributors add",
    "generate-contributor": "all-contributors generate",
    "build": "npm run build:server && npm run build:client",
    "build:client": "cross-env NODE_ENV=production webpack --env.prod",
    "build:server": "cross-env NODE_ENV=production babel src --out-dir tmp/lib",
    "prebuild": "npm run clean",
    "clean": "rimraf tmp lib",
    "dev": "npm-run-all --parallel dev:*",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --env.dev",
    "dev:server": "cross-env NODE_ENV=development nodemon --config nodemon.json src/server",
    "start": "cross-env NODE_ENV=production node tmp/lib/server"
  }
}
