{
  "name": "react-universal-starter",
  "version": "2.0.0",
  "main": "index.js",
  "author": "Bhargav Ponnapalli <bhargavponnapalli.5@gmail.com> (https://github.com/imbhargav5)",
  "license": "MIT",
  "scripts": {
    "add-contributor": "all-contributors add",
    "generate-contributor": "all-contributors generate",
    "build": "npm run build:server && npm run build:client",
    "build:client": "NODE_ENV=production webpack --mode production",
    "build:server": "NODE_ENV=production babel src --out-dir tmp/lib",
    "prebuild": "npm run clean",
    "clean": "rimraf tmp lib",
    "dev": "NODE_ENV=development nodemon --exec babel-node src/server",
    "start": "NODE_ENV=production node tmp/lib/server",
    "deploy": "now --public && now alias && now rm react-universal-starter --safe --yes"
  },
  "now": {
    "name": "react-universal-starter",
    "alias": "react-universal-starter"
  },
  "dependencies": {
    "express": "^4.15.4",
    "isomorphic-fetch": "^2.2.1",
    "prop-types": "^15.5.10",
    "pug": "^2.0.0-rc.3",
    "react": "^16.4.2",
    "react-dom": "16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "styled-components": "^2.1.2",
    "webpack-hot-middleware": "2.21.2"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.42",
    "@babel/core": "7.0.0-beta.42",
    "@babel/node": "7.0.0-beta.42",
    "@babel/preset-env": "7.0.0-beta.42",
    "@babel/preset-react": "7.0.0-beta.42",
    "@babel/preset-stage-0": "7.0.0-beta.42",
    "@babel/register": "7.0.0-beta.42",
    "all-contributors-cli": "^4.4.0",
    "babel-eslint": "8.2.2",
    "babel-loader": "8.0.0-beta.2",
    "babel-plugin-dynamic-import-webpack": "^1.0.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "6.26.0",
    "copy-webpack-plugin": "4.5.1",
    "cross-env": "^5.0.5",
    "eslint-plugin-react": "^7.3.0",
    "html-webpack-plugin": "3.0.6",
    "html-webpack-pug-plugin": "0.3.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "react-hot-loader": "next",
    "rimraf": "^2.6.1",
    "sw-precache-webpack-plugin": "0.11.5",
    "uglifyjs-webpack-plugin": "1.2.4",
    "webpack": "4.1.1",
    "webpack-cli": "2.0.12",
    "webpack-dev-middleware": "3.0.1"
  }
}
